- model_class = Documentation::Branch

= content_for :header do
  .head.pull-left
    %h1=t '.title', :default => model_class.model_name.human.titleize

%p
  %strong= model_class.human_attribute_name(:title) + ':'
  %br
  = @documentation_branch.title

= link_to t('.back', :default => t("helpers.links.back")), documentation_branches_path, :class => 'btn btn-default'
= link_to t('.edit', :default => t("helpers.links.edit")), edit_documentation_branch_path(@documentation_branch), :class => 'btn btn-default'
= link_to t('.destroy', :default => t("helpers.links.destroy")), documentation_branch_path(@documentation_branch), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
