.panel.panel-info
  .panel-heading.col-sm-6.col-sm-offset-3
    =select_tag('taxonomy_rov',options_for_select(taxonomies.map { |rov|[rov.title,rov.id,'data'=> widgets_bubbletree_path(rov.id) ] }) ,
          class: 'form-control taxonomy_select',id: name)
  .panel-body
    %iframe{ 'width' => '100%', 'height' => '850', src: widgets_bubbletree_path(taxonomies.first)}